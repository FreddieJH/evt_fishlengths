	author = {Hordyk, Adrian and Ono, Kotaro and Sainsbury, Keith and Loneragan, Neil and Prince, Jeremy},
	month = jan,
	year = {2015},
	pages = {204--216},
	file = {Hordyk et al (2015) Some explorations of the life history ratios to describe length composition,.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Hordyk et al (2015) Some explorations of the life history ratios to describe length composition,.pdf:application/pdf;Hordyk et al (2015) Some explorations of the life history ratios to describe length composition,.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Hordyk et al (2015) Some explorations of the life history ratios to describe length composition,2.pdf:application/pdf},
