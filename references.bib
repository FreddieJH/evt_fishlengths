	pages = {e1601198},
	file = {Ramírez et al (2017) Climate impacts on global hot spots of marine biodiversity.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Ramírez et al (2017) Climate impacts on global hot spots of marine biodiversity.pdf:application/pdf},
}

