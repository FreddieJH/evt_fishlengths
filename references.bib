	journal = {Journal of Memory and Language},
	author = {Barr, Dale J. and Levy, Roger and Scheepers, Christoph and Tily, Harry J.},
	month = apr,
	year = {2013},
	pages = {255--278},
	file = {Barr et al (2013) Random effects structure for confirmatory hypothesis testing.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Barr et al (2013) Random effects structure for confirmatory hypothesis testing.pdf:application/pdf},
