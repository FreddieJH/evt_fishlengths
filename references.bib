	file = {Tredennick et al (2021) A practical guide to selecting models for exploration, inference, and.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Tredennick et al (2021) A practical guide to selecting models for exploration, inference, and.pdf:application/pdf},
