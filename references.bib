	file = {Quince et al (2008) Biphasic growth in fish I.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Quince et al (2008) Biphasic growth in fish I.pdf:application/pdf},
