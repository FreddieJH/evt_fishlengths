	file = {Jacobsen et al (2014) The consequences of balanced harvesting of fish communities.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Jacobsen et al (2014) The consequences of balanced harvesting of fish communities.pdf:application/pdf},
