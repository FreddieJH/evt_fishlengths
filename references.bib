	pages = {20160147},
	file = {Kingsolver and Buckley (2017) Quantifying thermal extremes and biological variation to predict evolutionary.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Kingsolver and Buckley (2017) Quantifying thermal extremes and biological variation to predict evolutionary.pdf:application/pdf},
}

@article{barneche_warming_2019,
	title = {Warming increases the cost of growth in a model vertebrate},
	volume = {33},
	issn = {0269-8463, 1365-2435},
	url = {https://besjournals.onlinelibrary.wiley.com/doi/10.1111/1365-2435.13348},
	doi = {10.1111/1365-2435.13348},
	abstract = {Abstract
            
              
                
                  Growth rates directly influence individual fitness and constrain the flow of energy within food webs. Determining what factors alter the energetic cost of growth is therefore fundamental to ecological and evolutionary models.
                
                
                  
                    Here, we used theory to derive predictions about how the cost of growth varies over ontogeny and with temperature. We tested these predictions by measuring resting oxygen consumption and growth rates of zebrafish (
                    Danio rerio
                    ) at four temperature treatments (20°C, 26°C, 29°C, 32°C). We calibrated oxygen consumption for ATP production by measuring the efficiency of mitochondria (P/O ratios) at the end of the experiment.
                  
                
                
                  We show that, when compared to growth rates, rates of resting oxygen consumption exhibited a significantly higher temperature dependence and significantly lower mass scaling. P/O ratios varied substantially among individuals, but did not account for the observed difference in temperature dependence between rates. Consequently, the cost of growth was significantly higher at earlier ontogenetic stages, and it increased twofold between 20°C and 32°C. Temperature systematically decreased the size of adults between 26°C and 32°C.
                
                
                  To the best of our knowledge, our study represents the first attempt to characterise how the cost of growth varies over ontogeny and across temperatures in a model vertebrate. Rising temperatures will substantially alter the structure of ecosystems and the distribution of biomass in food webs because they drive a decline in the average size of individuals, and because higher costs of growth lead to inefficient energy transfer between trophic levels.
                
              
            
            
              A
              plain language summary
              is available for this article.},
	language = {en},
	number = {7},
	urldate = {2024-01-10},
	journal = {Functional Ecology},
	author = {Barneche, Diego R. and Jahn, Miki and Seebacher, Frank},
	editor = {Smallegange, Isabel},
	month = jul,
	year = {2019},
	pages = {1256--1266},
	file = {Barneche et al (2019) Warming increases the cost of growth in a model vertebrate.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Barneche et al (2019) Warming increases the cost of growth in a model vertebrate.pdf:application/pdf},
