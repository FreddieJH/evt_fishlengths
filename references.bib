	number = {41},
	urldate = {2022-07-25},
	journal = {Proceedings of the National Academy of Sciences},
	author = {McClanahan, Tim R. and Graham, Nicholas A. J. and MacNeil, M. Aaron and Muthiga, Nyawira A. and Cinner, Joshua E. and Bruggemann, J. Henrich and Wilson, Shaun K.},
