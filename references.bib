	file = {Sandin et al (2008) Baselines and Degradation of Coral Reefs in the Northern Line Islands.PDF:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Sandin et al (2008) Baselines and Degradation of Coral Reefs in the Northern Line Islands.PDF:application/pdf},
