	journal = {Journal of Applied Ecology},
	author = {Goetze, Jordan S. and Claudet, Joachim and Januchowski‐Hartley, Fraser and Langlois, Timothy J. and Wilson, Shaun K. and White, Crow and Weeks, Rebecca and Jupiter, Stacy D.},
	editor = {Trenkel, Verena},
	month = may,
	year = {2018},
