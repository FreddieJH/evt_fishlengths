	pages = {e2304404120},
	file = {Perret et al (2024) A species’ response to spatial climatic variation does not predict its response.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Perret et al (2024) A species’ response to spatial climatic variation does not predict its response.pdf:application/pdf},
