@article{nagelkerken_trophic_2020,
	title = {Trophic pyramids reorganize when food web architecture fails to adjust to ocean change},
	volume = {369},
	issn = {0036-8075, 1095-9203},
	url = {https://www.sciencemag.org/lookup/doi/10.1126/science.aax0621},
	doi = {10.1126/science.aax0621},
	abstract = {As human activities intensify, the structures of ecosystems and their
                        food webs often reorganize. Through the study of mesocosms harboring a
                        diverse benthic coastal community, we reveal that food web architecture can
                        be inflexible under ocean warming and acidification and unable to compensate
                        for the decline or proliferation of taxa. Key stabilizing processes,
                        including functional redundancy, trophic compensation, and species
                        substitution, were largely absent under future climate conditions. A trophic
                        pyramid emerged in which biomass expanded at the base and top but contracted
                        in the center. This structure may characterize a transitionary state before
                        collapse into shortened, bottom-heavy food webs that characterize ecosystems
                        subject to persistent abiotic stress. We show that where food web
                        architecture lacks adjustability, the adaptive capacity of ecosystems to
                        global change is weak and ecosystem degradation likely.},
	language = {en},
	number = {6505},
	urldate = {2022-07-25},
	journal = {Science},
	author = {Nagelkerken, Ivan and Goldenberg, Silvan U. and Ferreira, Camilo M. and Ullah, Hadayet and Connell, Sean D.},
