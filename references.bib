	pages = {327--334},
	file = {McClain (2004) Connecting species richness, abundance and body size in deep-sea gastropods.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\McClain (2004) Connecting species richness, abundance and body size in deep-sea gastropods.pdf:application/pdf},
