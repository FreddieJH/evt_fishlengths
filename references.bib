	file = {Deutsch et al (2022) Impact of warming on aquatic body sizes explained by metabolic scaling from.pdf:C\:\\Users\\freddieh\\OneDrive - University of Tasmania\\Documents\\Work\\Papers\\Deutsch et al (2022) Impact of warming on aquatic body sizes explained by metabolic scaling from.pdf:application/pdf},
