@article{hilborn_effective_2020,
	title = {Effective fisheries management instrumental in improving fish stock status},
	volume = {117},
	issn = {0027-8424, 1091-6490},
	url = {https://pnas.org/doi/full/10.1073/pnas.1909726116},
