@article{barnes_global_2010,
	title = {Global patterns in predator–prey size relationships reveal size dependency of trophic transfer efficiency},
	volume = {91},
	copyright = {http://onlinelibrary.wiley.com/termsAndConditions\#vor},
	issn = {0012-9658, 1939-9170},
