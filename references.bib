@article{englund_temperature_2011,
	title = {Temperature dependence of the functional response},
	volume = {14},
	issn = {1461-023X, 1461-0248},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1461-0248.2011.01661.x},
